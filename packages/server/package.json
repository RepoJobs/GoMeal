{
  "name": "server",
  "version": "0.0.1",
  "description": "API",
  "author": "gabrieldev525 <gvictor525.gv@gmail.com>",
  "homepage": "https://github.com/RepoJobs/GoMeal#readme",
  "license": "ISC",
  "scripts": {
    "test": "npm run tsc -- --noEmit && jest",
    "dev": "nodemon src/server.ts",
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "start": "npm run build && node dist/server.js",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:fix": "eslint . --ext .ts --fix && npx prettier --write .",
    "lint:eslint": "eslint . --ext .ts",
    "lint:prettier": "npx prettier --check .",
    "tsc": "tsc",
    "prisma:merge": "npx prismerge -i prismerge.json",
    "prisma:migrate": "npx prisma migrate dev"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "module-alias": "^2.2.2",
    "ts-node-dev": "^2.0.0"
  },
  "devDependencies": {
    "@prisma-utils/prismerge": "^1.6.2",
    "@swc/core": "^1.2.215",
    "@swc/jest": "^0.2.21",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.5",
    "@types/module-alias": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "nodemon": "^2.0.20",
    "jest": "^28.1.3",
    "prettier": "2.7.1",
    "prisma": "^4.1.0"
  },
  "prisma": {
    "schema": "src/infra/db/prisma/schema.prisma"
  }
}
